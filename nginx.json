{
    "family": "nginx",
    "containerDefinitions": [
        {
	    "image": "denisdevel/nginx:v_%BUILD_NUMBER%",
            "name": "nginx",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
